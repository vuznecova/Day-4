//Write a Java program to create a new array list, add some colors (string) and print out the collection.

import java.util.*;
public class Main {
  public static void main(String[] args) {
  List<String> arrL = new ArrayList<String>() ;
  arrL.add("Red");
  arrL.add("Orange");
  arrL.add("Yellow");
  arrL.add("Green");
  arrL.add("Blue");
  arrL.add("Purple");
  System.out.println(arrL);
 }
}
