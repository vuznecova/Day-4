//Write a Java program to create a reverse order view of the elements contained in a given tree set.

import java.util.*;
public class Main {
  public static void main(String[] args) {
  TreeSet<String> treeS = new TreeSet<String>() ;
  treeS.add("Red");
  treeS.add("Orange");
  treeS.add("Yellow");
  treeS.add("Green");
  treeS.add("Blue");
  treeS.add("Purple");
  
  System.out.println("Your Hash Set: " + treeS);
  Iterator it = treeS.descendingIterator();
  System.out.print("Reverse order: ");
  while (it.hasNext()) {
        System.out.print(it.next() + " ");
     }
  }
 }
